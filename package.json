    "inversify-express-utils": "^6.5.0",
    "jest": "^29.7.0",
    "node-fetch": "^2.7.0",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.1",
    "sqlite": "^5.1.1",
    "sqlite3": "^5.1.7",
    "uuid": "^11.1.0",
    "yup": "^1.6.1"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/cors": "^2.8.17",
